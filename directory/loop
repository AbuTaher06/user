radius = 23% meters
calculateType = 'Volume'
switch calculateType
    case 'Perimeter'
        perimeter = 2*pi*radius
    case 'Area'
        area = pi*radius^2
    case 'Volume'
        volume = 4*pi*radius^3/3
end




sideLength = 6; % meters
radius = 3.14;
width = 5;
length = 7;

areaType = 'Square';
switch areaType
    case 'Square'
        area = sideLength^2
    case 'Circle'
        area = pi*radius^2;
    case 'Rectangle'
        area = width*length;
end
	
	area = 36
